---
layout: base.njk
---
<main id="awaktama">
  <article>
    <p>
      [hah]
      
    {%- for nak in list -%}
      {{ nak.nama }} dan {{ nak.banda }} <br>
    {%- endfor %}
    </p>
    Blog / {{ category }} /
    <h2>
      {{ title }}
    </h2>
    <div id="awalan">
      <li>
        by <a href="{{ authorLink }}">{{ author }}</a>
      </li>
      <li>
        in {{ date }}
      </li>
      <li>
        on 
        <ul>
          {%- for cat in labels -%}
          <li>
            #{{ cat }} 
          </li>
          {%- endfor %}
        </ul>
      </li>
    </div>
    <p id="deskripsitama">
      {{ description }}
    </p>
    <img src="" alt="">
  
    <div id="wadah-blog">
      {{ content | safe }}
    </div>
  </article>
</main>